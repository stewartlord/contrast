{
  "name": "Contrast",
  "version": "0.2.0",
  "description": "A git diff tool",
  "author": "Stewart Lord",
  "license": "MIT",
  "homepage": "https://github.com/stewartlord/contrast#readme",
  "repository": "https://github.com/stewartlord/contrast.git",
  "keywords": [
    "Contrast",
    "diff",
    "electron"
  ],
  "main": "main.js",
  "scripts": {
    "start": "electron . --dev",
    "pack": "build --dir",
    "dist": "build"
  },
  "bugs": {
    "url": "https://github.com/stewartlord/contrast/issues"
  },
  "postinstall": "install-app-deps",
  "dependencies": {
    "diff": "^3.2.0",
    "font-awesome": "^4.7.0",
    "highlights": "^3.0.0",
    "jquery": "^3.2.0",
    "nodegit": "^0.20.3",
    "vue": "^2.3.3",
    "vuex": "^2.3.1",
    "vuex-persistedstate": "^2.0.0"
  },
  "devDependencies": {
    "electron": "^1.7.10",
    "electron-builder": "^18.7.0",
    "electron-rebuild": "^1.7.3"
  },
  "build": {
    "appId": "contrast",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "dmg": {
      "contents": [
        {
          "type": "file",
          "x": 135,
          "y": 265
        },
        {
          "type": "link",
          "path": "/Applications",
          "x": 405,
          "y": 265
        }
      ]
    }
  }
}
